<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".GetContent">

    <ImageView
        android:layout_width="35dp"
        android:id="@+id/setting"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"
        android:layout_gravity="right"
        android:layout_height="35dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingLeft="20dp"
        android:paddingTop="30dp"
        android:layout_marginLeft="10dp"
        android:gravity="center"
        android:layout_gravity="center"
        android:layout_marginRight="10dp"
        android:paddingBottom="30dp"
        android:paddingRight="20dp"
        android:layout_height="match_parent">



        <Button
            android:layout_width="match_parent"
            android:layout_marginBottom="10dp"
            android:textColor="#ffffff"
            android:text="50문답 확인"
            android:stateListAnimator="@null"
            android:id="@+id/btn_self50_info"
            android:fontFamily="@font/binggrae_bold"
            android:background="@drawable/btn_pink_click"
            android:layout_height="wrap_content" />

        <Button
            android:layout_width="match_parent"
            android:layout_marginBottom="10dp"
            android:stateListAnimator="@null"
            android:textColor="#ffffff"
            android:text="100문답 확인"
            android:id="@+id/btn_self100_info"
            android:fontFamily="@font/binggrae_bold"
            android:background="@drawable/btn_pink_click"
            android:layout_height="wrap_content" />

        <Button
            android:layout_width="match_parent"
            android:layout_marginBottom="10dp"
            android:textColor="#ffffff"
            android:text="상대방이 만든 50문답지 답변"
            android:id="@+id/btn_you50_info"
            android:stateListAnimator="@null"
            android:fontFamily="@font/binggrae_bold"
            android:background="@drawable/btn_pink_click"
            android:layout_height="wrap_content" />

        <Button
            android:layout_width="match_parent"
            android:layout_marginBottom="10dp"
            android:textColor="#ffffff"
            android:text="상대방이 만든 100문답지 답변"
            android:fontFamily="@font/binggrae_bold"
            android:id="@+id/btn_you100_info2"
            android:stateListAnimator="@null"
            android:background="@drawable/btn_pink_click"
            android:layout_height="wrap_content" />

        <Button
            android:layout_width="match_parent"
            android:layout_marginBottom="10dp"
            android:textColor="#ffffff"
            android:text="내가만든 50문답지 답변확인"
            android:id="@+id/btn_you50_info2"
            android:stateListAnimator="@null"
            android:fontFamily="@font/binggrae_bold"
            android:background="@drawable/btn_pink_click"
            android:layout_height="wrap_content" />

        <Button
            android:layout_width="match_parent"
            android:layout_marginBottom="10dp"
            android:textColor="#ffffff"
            android:text="내가 만든 100문답지 답변확인"
            android:stateListAnimator="@null"
            android:id="@+id/btn_you100_give"
            android:fontFamily="@font/binggrae_bold"
            android:background="@drawable/btn_pink_click"
            android:layout_height="wrap_content" />

        <Button
            android:layout_width="match_parent"
            android:layout_marginBottom="10dp"
            android:textColor="#ffffff"
            android:text="버킷리스트10 확인"
            android:fontFamily="@font/binggrae_bold"
            android:id="@+id/btn_bucket10"
            android:stateListAnimator="@null"
            android:background="@drawable/btn_pink_click"
            android:layout_height="wrap_content" />

        <Button
            android:layout_width="match_parent"
            android:textColor="#ffffff"
            android:text="버킷리스트30 확인"
            android:fontFamily="@font/binggrae_bold"
            android:stateListAnimator="@null"
            android:id="@+id/bucket30_move"
            android:layout_marginBottom="10dp"
            android:background="@drawable/btn_pink_click"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="match_parent"
            android:gravity="center"
            android:id="@+id/text_logout"
            android:text="로그아웃"
            android:textColor="#757575"
            android:layout_height="wrap_content" />

    </LinearLayout>


</LinearLayout>